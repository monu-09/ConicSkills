<?xml version="1.0" encoding="utf-8"?> <!-- UNSELECTED TAB STATE -->
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Bottom indicator color for the UNSELECTED tab state -->
    <item
        android:left="-5dp"
        android:right="-5dp"
        android:top="-5dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="2dp"
                android:color="#65acee" />
        </shape>
    </item>
</layer-list>
